/* CSS for constraint tests - minimal rules for testing constraint properties */

/* Base styles (no background on * to allow transparency) */
* {
    color: white;
}

/* Height constraints */
.height-fixed {
    height: 3;
}

.height-fit {
    height: fit;
}

/* Width constraints */
.width-fixed {
    width: 10;
}

.width-fit {
    width: fit;
}

.width-percent {
    width: 25%;
}

/* Parent-child selector tests */
.parent-row {
    flex: center;
}

.parent-row .child-text {
    width: fit;
}
